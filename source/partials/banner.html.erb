<div class="row justify-content-center banner">
  <div class="col-12 d-flex flex-column justify-content-center align-items-center h-100">
    <div class="container" id="banner-container">
      <%= image_tag 'photo_me.png', alt: "profile photo", class: 'avatar-large' %>
      <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.1/jquery.min.js"></script>
      <h1 class="title-main" id="mainTitle"></h1>
      <script>
        new TypeIt("#mainTitle", {
          strings: "Ayaka Inaba's Portfolio",
          speed: 50,
          waitUntilVisible: true,
        }).go();
      </script>
    </div>
  </div>
</div>
